<?xml version="1.0" encoding="UTF-8"?>
<Page id = "itemimages" showinFrame ="true"  title ="Sales Target Analysis| Rainbow CRM" Object = "com.rainbow.crm.salesperiod.model.SalesPeriodAnalyzer" 
Controller = "com.rainbow.crm.salesperiod.controller.SalesPeriodAnalyzerController">
<Template type= "General"  name = "GraphTemplate"></Template>

<Elements>
<UIForm Id ="cust" method = "POST" enctype="multipart/form-data" >
<Section type ="topBar"></Section> 
<Section type ="leftsideBarApp"></Section>
<CoreSection>
<ErrorSection>
</ErrorSection>
<Section type= "mainSection">
<Element type = "UITable" rows = "1" cols = "2"  style = "outlinedTable">
	<Element type = "UITableRow">
	<Element type ="UITableCol"><Element label = "Sales_Period"  type = "UIList" Id = "txtcode"    property ="salePeriod"  >
		<options populator =  "getSalePeriods"> 
		</options>
	 </Element>
	 </Element>
	 <Element type ="UITableCol">
	 	<Element caption = "Analyze"  type = "UIButton" Id = "btnFilterCleas"  style="button" fixedAction ="FixedAction.ACTION_READ"   />
	 </Element>
</Element>
</Element>
</Section>
<Section type= "displaySection">
	<Element type = "UIBarChart" Id = "brChrt"  dataProvider = "salesData" >
	  <xStart>0</xStart>
	  <yStart>20</yStart>
	  <rangeHeight>600</rangeHeight>
	  <rangeWidth>1600</rangeWidth>
	  <marginWidth>30</marginWidth>
	  <barWidth>30</barWidth>
	  <noYAxisDivisions>6</noYAxisDivisions>
	</Element>
</Section>
</CoreSection>
<Section type ="copyRightSection"/>
</UIForm>
</Elements>
</Page>
